{
    "name": "owmods-gui-frontend",
    "private": true,
    "version": "0.12.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "devtools": "react-devtools --in-process-gpu",
        "gen-types": "typeshare ../.. --lang=typescript --output-file=src/types.d.ts && prettier --write src/types.d.ts",
        "build": "tsc && vite build --emptyOutDir",
        "preview": "vite preview",
        "check": "pnpm lint && pnpm prettify",
        "lint": "tsc --noEmit && eslint --cache . --fix",
        "lint-dry": "tsc --noEmit && eslint .",
        "prettify": "prettier --write . --ignore-unknown",
        "format": "pnpm prettify",
        "prettier-check": "prettier --check . --ignore-unknown",
        "preinstall": "npx only-allow pnpm"
    },
    "dependencies": {
        "@emotion/react": "11.11.3",
        "@emotion/styled": "11.11.0",
        "@fontsource/roboto": "5.0.8",
        "@mui/icons-material": "5.15.2",
        "@mui/lab": "5.0.0-alpha.158",
        "@mui/material": "5.15.2",
        "@tauri-apps/api": "2.0.0-alpha.13",
        "@tauri-apps/plugin-app": "2.0.0-alpha.1",
        "@tauri-apps/plugin-dialog": "2.0.0-alpha.5",
        "@tauri-apps/plugin-os": "2.0.0-alpha.6",
        "@tauri-apps/plugin-shell": "2.0.0-alpha.5",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-error-boundary": "4.0.12",
        "react-virtuoso": "4.6.2"
    },
    "devDependencies": {
        "@testing-library/react": "14.1.2",
        "@types/react": "18.2.46",
        "@types/react-dom": "18.2.18",
        "@typescript-eslint/eslint-plugin": "6.16.0",
        "@typescript-eslint/parser": "6.16.0",
        "@vitejs/plugin-react": "4.2.1",
        "eslint": "8.56.0",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "prettier": "3.1.1",
        "typescript": "5.3.3",
        "vite": "5.0.10",
        "vite-imagetools": "6.2.8"
    }
}
