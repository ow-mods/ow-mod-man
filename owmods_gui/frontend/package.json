{
    "name": "owmods-gui-frontend",
    "private": true,
    "version": "0.14.2",
    "type": "module",
    "engines": {
        "npm": ">=10.0.0",
        "node": ">=20.0.0"
    },
    "scripts": {
        "dev": "vite",
        "devtools": "react-devtools --in-process-gpu",
        "gen-types": "typeshare ../.. --lang=typescript --output-file=src/types.d.ts && prettier --write src/types.d.ts",
        "build": "tsc && vite build --emptyOutDir",
        "preview": "vite preview",
        "lint": "tsc --noEmit && eslint --cache . --fix",
        "lint-dry": "tsc --noEmit && eslint .",
        "prettify": "prettier --write . --ignore-unknown",
        "prettier-check": "prettier --check . --ignore-unknown",
        "tauri": "cd ../backend && tauri"
    },
    "dependencies": {
        "@emotion/react": "11.13.0",
        "@emotion/styled": "11.13.0",
        "@fontsource/roboto": "5.0.14",
        "@mui/icons-material": "5.16.6",
        "@mui/lab": "5.0.0-alpha.173",
        "@mui/material": "5.16.6",
        "@tauri-apps/api": "2.0.1",
        "@tauri-apps/plugin-clipboard-manager": "2.0.0",
        "@tauri-apps/plugin-dialog": "2.0.0",
        "@tauri-apps/plugin-os": "2.0.0",
        "@tauri-apps/plugin-process": "2.0.0",
        "@tauri-apps/plugin-shell": "2.0.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-error-boundary": "4.0.13",
        "react-virtuoso": "4.9.0"
    },
    "devDependencies": {
        "@tauri-apps/cli": "^2.0.1",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@typescript-eslint/eslint-plugin": "7.18.0",
        "@typescript-eslint/parser": "7.18.0",
        "@vitejs/plugin-react": "4.3.1",
        "eslint": "8.57.0",
        "eslint-plugin-react": "7.35.0",
        "eslint-plugin-react-hooks": "4.6.2",
        "prettier": "3.3.3",
        "typescript": "5.5.4",
        "vite": "5.4.0",
        "vite-imagetools": "7.0.4"
    }
}
